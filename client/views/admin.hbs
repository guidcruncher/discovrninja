 <nav>
   <div class="nav nav-tabs" id="nav-tab" role="tablist">
     <button class="nav-link active" id="nav-container-tab" data-bs-toggle="tab" data-bs-target="#nav-container" type="button" role="tab" aria-controls="nav-container" aria-selected="true">Container</button>
     <button class="nav-link" id="nav-projects-tab" data-bs-toggle="tab" data-bs-target="#nav-projects" type="button" role="tab" aria-controls="nav-projects" aria-selected="false">Projects</button>
     <button class="nav-link" id="nav-compose-tab" data-bs-toggle="tab" data-bs-target="#nav-compose" type="button" role="tab" aria-controls="nav-compose" aria-selected="false">Composerise</button>
     <button class="nav-link" id="nav-monitor-tab" data-bs-toggle="tab" data-bs-target="#nav-monitor" type="button" role="tab" aria-controls="nav-monitor" aria-selected="false">Monitor</button>
   </div>
 </nav>
 <div class="tab-content" id="nav-tabContent">
   <div class="tab-pane fade show active" id="nav-container" role="tabpanel" aria-labelledby="nav-container-tab" tabindex="0">
     <div id="hbsps" class="hbs-template" data-url="/api/docker/ps" data-template="dockerps" data-refresh-interval="10"></div>
   </div>
   <div class="tab-pane fade" id="nav-projects" role="tabpanel" aria-labelledby="nav-projects-tab" tabindex="0">
     <div id="hbsprojects" class="hbs-template" data-url="/api/docker/projects" data-template="projects"></div>
   </div>
   <div class="tab-pane fade" id="nav-compose" role="tabpanel" aria-labelledby="nav-compose-tab" tabindex="0">
     {{>compose}}
   </div>
   <div class="tab-pane fade" id="nav-monitor" role="tabpanel" aria-labelledby="nav-monitor-tab" tabindex="0">
     <h1>
       <div id="monitorContainerName">Container</div>
     </h1>
     <table border="0" cellpadding="0" cellspacing="5">
       <tr>
         <td>Historical CPU Use %</td>
       </tr>
       <tr>
         <td>
           <div id="cpuchart" style="width:600px;height:300px;"></div>
         </td>
       </tr>
       <tr>
         <td>Historical Memory Free %</td>
       </tr>
       <tr>
         <td>
           <div id="memorychart" style="width:600px;height:300px;"></div>
         </td>
       </tr>
     </table>
     <div id="containerLogs" class="terminal">
     </div>
   </div>
 </div>
 {{>messagebox}}
 {{>iconsearch}}
 {{>search}}
 <div id="containerInfo" data-url="/api/docker/container/{id}" data-template="containerinfo">
 </div>
