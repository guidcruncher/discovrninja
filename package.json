{
  "name": "discovrninja",
  "version": "0.0.245",
  "description": "The DiscovrNinja service",
  "author": "guidcruncher",
  "private": true,
  "license": "GNU",
  "scripts": {
    "shell": "docker exec -i -t discovrninja_dev sh",
    "stop": "docker compose stop && docker compose rm -f",
    "restart": "docker compose restart",
    "logs": "docker compose logs -f",
    "helpers": "/home/node/buildscripts/mergehelpers.sh > /home/node/src/client/public/js/lib/090-helpers.js",
    "prebuild": "npx rimraf ./src/client/public/img/bing.jpg ./dist && mkdir -p ./dist/client/public/js ./dist/client/public/css ./dist/client/public/img ./dist/client/public/weather",
    "dev": "npm run helpers && npx nest build && npm run uglify && npm run uglifycss && node ",
    "buildprod": "npx nest build",
    "format": "npx prettier --write \"src/**/*.ts\" ",
    "start:debug": "npx nest start --debug --watch",
    "lint": "npx eslint -c ./eslint.config.mjs '{src,apps,libs}/**/*.ts' --fix",
    "lint-client": "npx eslint -c ./eslint.config.client.mjs src/client/**/*.js --fix",
    "uglify": "npx uglifyjs ./src/client/public/js/lib/*.js -o ./dist/client/public/js/main.js && npx uglifyjs ./dist/client/public/js/main.js -c unused=false -m --source-map -o ./dist/client/public/js/main.min.js",
    "uglifycss": "npx uglifycss ./src/client/public/css/lib/*.css --output ./dist/client/public/css/style.css"
  },
  "dependencies": {
    "@fastify/compress": "^8.0.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/helmet": "^13.0.1",
    "@fastify/secure-session": "^8.1.1",
    "@fastify/static": "^8.1.1",
    "@fastify/view": "^10.0.2",
    "@nestjs/common": "^11.0.10",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.10",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mongoose": "^11.0.1",
    "@nestjs/platform-express": "^11.0.10",
    "@nestjs/platform-fastify": "^11.0.10",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^11.0.4",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "composerize": "^1.7.1",
    "cookie": "^1.0.2",
    "dclint": "^2.2.2",
    "decomposerize": "^1.4.4",
    "docker-compose": "^1.1.1",
    "dockerode": "^4.0.4",
    "fancy-ansi": "^0.1.3",
    "feedparser": "^2.2.10",
    "handlebars": "^4.7.8",
    "js-yaml": "^4.1.0",
    "mongoose": "^8.10.1",
    "nestjs-pino": "^4.3.1",
    "openmeteo": "^1.1.4",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "reflect-metadata": "^0.2.0",
    "rss-parser": "^3.13.0",
    "rxjs": "^7.8.2",
    "showdown": "^2.1.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@nestjs/cli": "^11.0.4",
    "@nestjs/schematics": "^11.0.1",
    "@nestjs/testing": "^11.0.10",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.18",
    "@types/dockerode": "^3.3.34",
    "@types/express": "^5.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.13.5",
    "@types/supertest": "^6.0.0",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "gulp": "^5.0.0",
    "gulp-change": "^1.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-declare": "^0.3.0",
    "gulp-execa": "^8.0.0",
    "gulp-handlebars": "^5.0.2",
    "gulp-replace": "^1.1.4",
    "gulp-wrap": "^0.15.0",
    "js-beautify": "^1.15.3",
