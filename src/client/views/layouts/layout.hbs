<body>
  {{# when size.window.w "gte" 768}}
    {{> headerwide }}
    {{else}}
      {{>header}}
      {{/when}}

      <div class="container" style="margin-top:15px">
        {{{body}}}
      </div>

      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
      <script type="text/javascript">
        ui().desktop();
        ui().themeChooser().setTheme();
        ui(".hbs-template").template();

      </script>
      {{>messagebox}}

      <script type="text/javascript">
        $(document).ready(function () {
          $('.datatable').DataTable({
            paging: false,
            searching: false
          });
        });

      </script>
